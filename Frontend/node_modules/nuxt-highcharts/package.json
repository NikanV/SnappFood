{
  "name": "nuxt-highcharts",
  "version": "2.0.4",
  "description": "Highcharts for Nuxt",
  "repository": "richardeschloss/nuxt-highcharts",
  "license": "MIT",
  "type": "module",
  "contributors": [
    {
      "name": "Richard Schloss <richard.e.schloss@protonmail.com>"
    }
  ],
  "imports": {
    "#root/*": "./*"
  },
  "files": [
    "lib"
  ],
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "test:watch": "ava --color --watch",
    "test:cov": "c8 ava --timeout=10m"
  },
  "dependencies": {
    "highcharts": "10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@nuxtjs/eslint-config": "^6.0.1",
    "ava": "^3.15.0",
    "bootstrap-vue": "^2.21.2",
    "browser-env": "^3.3.0",
    "c8": "^7.10.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-unicorn": "^37.0.1",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "nuxt": "^2.15.8"
  },
  "optionalDependencies": {
    "@highcharts/map-collection": "^2.0.0",
    "proj4": "^2.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nuxt",
    "highcharts",
    "vue",
    "vuejs"
  ]
